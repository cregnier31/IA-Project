pipeline {
    agent any
    stages {
        stage('Construction') {
            steps {
                echo 'construction encours'
            }
        }
        stage('Test') {
            steps {
                echo 'Debut du test'
                sleep 2
                echo 'Fin du test'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploiment'
            }
        }
    }
}
